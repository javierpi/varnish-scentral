<?xml version="1.0" encoding="UTF-8"?>
<config>
    <proyecto>
        <destination_node>cepal_project</destination_node>
        <xml_file_path>/home/francort/xml/proyecto.xml</xml_file_path>
        <x_path>/GetProyectos/proyecto</x_path>
        <id_x_path>id_protrack</id_x_path>
        <service_endpoint>http://10.0.17.152/drupal_protrack/service1.asmx/GetProyectos</service_endpoint>
        <service_parameters>
            <service_parameter>
                <name>num_records</name>
                <value></value>
            </service_parameter>
            <service_parameter>
                <name>id_proyecto</name>
                <value></value>
            </service_parameter>
            <service_parameter>
                <name>no_condition</name>
                <value>false</value>
            </service_parameter>
        </service_parameters>
    </proyecto>
    <comunicado>
        <destination_node>cepal_pr</destination_node>
        <xml_file_path>/home/francort/xml/comunicado.xml</xml_file_path>
        <x_path>/GetPressReleases/pressrelease</x_path>
        <id_x_path>id_sade</id_x_path>
        <service_endpoint>http://10.0.17.152/sade_ws/service1.asmx/GetPressReleases</service_endpoint>
        <service_parameters>
            <service_parameter>
                <name>num_records</name>
                <value></value>
            </service_parameter>
            <service_parameter>
                <name>id_sade</name>
                <value></value>
            </service_parameter>
            <service_parameter>
                <name>solo_ids</name>
                <value>false</value>
            </service_parameter>
        </service_parameters>
        <destination_images>public://pr/images</destination_images>
        <source_images>/home/francort/imagenes_comunicados</source_images>
        <destination_files>public://pr/files</destination_files>
        <source_files>/home/francort/archivos_comunicados</source_files>
    </comunicado>
    <noticia>
        <destination_node>cepal_news</destination_node>
        <xml_file_path>/home/francort/xml/noticia.xml</xml_file_path>
        <x_path>/GetNews/noticia</x_path>
        <id_x_path>id_sade</id_x_path>
        <service_endpoint>http://10.0.17.152/sade_ws/service1.asmx/GetNoticias</service_endpoint>
        <service_parameters>
            <service_parameter>
                <name>num_records</name>
                <value>200</value>
            </service_parameter>
            <service_parameter>
                <name>id_sade</name>
                <value></value>
            </service_parameter>
        </service_parameters>
        <files_service>
            <service_endpoint>http://10.0.17.152/sade_ws/service1.asmx/GetNewsFile</service_endpoint>
        </files_service>
        <images_service>
            <service_endpoint>http://10.0.17.152/sade_ws/service1.asmx/GetNewsImage</service_endpoint>
        </images_service>
        <destination_images>public://news/images</destination_images>
        <destination_files>public://news/files</destination_files>
        <source_files>/home/francort/archivos_noticias</source_files>
        <source_images>/home/francort/imagenes_noticias</source_images>
        <xml_images_path>/home/francort/xml/imagenes_noticia.xml</xml_images_path>
        <xml_files_path>/home/francort/xml/archivos_noticia.xml</xml_files_path>
    </noticia>
    <articulo>
        <destination_node>cepal_article</destination_node>
        <xml_file_path>/home/francort/xml/noticia.xml</xml_file_path>
        <x_path>/GetArticulos/articulo</x_path>
        <id_x_path>id_sade</id_x_path>
        <service_endpoint>http://10.0.17.152/sade_ws/service1.asmx/GetArticulos</service_endpoint>
        <service_parameters>
            <service_parameter>
                <name>id_sade</name>
                <value></value>
            </service_parameter>
        </service_parameters>
    </articulo>
    <imagen>
        <destination_node>image</destination_node>
        <xml_file_path>/home/francort/xml/imagenes_comunicados.xml</xml_file_path>
        <x_path>/GetPressReleaseImage/image</x_path>
        <id_x_path>id_foto</id_x_path>
        <service_endpoint>http://10.0.17.152/sade_ws/service1.asmx/GetPressReleaseImage</service_endpoint>
        <service_parameters>
            <service_parameter>
                <name>id_sade</name>
                <value></value>
            </service_parameter>
            <service_parameter>
                <name>num_records</name>
                <value></value>
            </service_parameter>
        </service_parameters>
    </imagen>

    <user>
        <xml_file_path>/home/francort/xml/user.xml</xml_file_path>
        <x_path>/GetUsuarios/usuario</x_path>
        <id_x_path>uid</id_x_path>
    </user>

    <imagenesComunicado>
        <xml_file_path>/home/francort/xml/imagenes_comunicado.xml</xml_file_path>
    </imagenesComunicado>
    <archivosComunicado>
        <xml_file_path>/home/francort/xml/archivos_comunicado.xml</xml_file_path>
    </archivosComunicado>

    <imagenesNoticia>
        <xml_file_path>/home/francort/xml/imagenes_noticia.xml</xml_file_path>
    </imagenesNoticia>
    <archivosNoticia>
        <xml_file_path>/home/francort/xml/archivos_noticia.xml</xml_file_path>
    </archivosNoticia>
</config>