<?xml version="1.0" encoding="UTF-8"?>
<config>
    <proyecto>
        <destination_node>cepal_project</destination_node>
        <xml_file_path>/home/francort/xml/proyecto.xml</xml_file_path>
        <x_path>/GetProyectos/proyecto</x_path>
        <id_x_path>id_protrack</id_x_path>
        <service_endpoint>http://10.0.17.152/drupal_protrack/service1.asmx/GetProyectos</service_endpoint>
        <service_parameters>
            <service_parameter>
                <name>num_records</name>
                <value></value>
            </service_parameter>
            <service_parameter>
                <name>id_proyecto</name>
                <value></value>
            </service_parameter>
            <service_parameter>
                <name>no_condition</name>
                <value>false</value>
            </service_parameter>
        </service_parameters>
    </proyecto>
    <comunicado>
        <destination_node>cepal_pr</destination_node>
        <xml_file_path>/home/francort/xml/comunicado.xml</xml_file_path>
        <x_path>/GetPressReleases/pressrelease</x_path>
        <id_x_path>id_sade</id_x_path>
        <service_endpoint>http://10.0.17.152/sade_ws/service1.asmx/GetPressReleases</service_endpoint>
        <service_parameters>
            <service_parameter>
                <name>num_records</name>
                <value></value>
            </service_parameter>
            <service_parameter>
                <name>id_sade</name>
                <value></value>
            </service_parameter>
            <service_parameter>
                <name>solo_ids</name>
                <value>false</value>
            </service_parameter>
        </service_parameters>
    </comunicado>
    <noticia>
        <destination_node>cepal_news</destination_node>
        <xml_file_path>/home/francort/xml/noticia.xml</xml_file_path>
        <x_path>/GetNoticias/noticia</x_path>
        <id_x_path>id_sade</id_x_path>
        <service_endpoint>http://10.0.17.152/sade_ws/service1.asmx/GetNoticias</service_endpoint>
        <service_parameters>
            <service_parameter>
                <name>num_records</name>
                <value>200</value>
            </service_parameter>
            <service_parameter>
                <name>id_sade</name>
                <value></value>
            </service_parameter>
        </service_parameters>
    </noticia>
    <articulo>
        <destination_node>cepal_article</destination_node>
        <xml_file_path>/home/francort/xml/noticia.xml</xml_file_path>
        <x_path>/GetArticulos/articulo</x_path>
        <id_x_path>id_sade</id_x_path>
        <service_endpoint>http://10.0.17.152/sade_ws/service1.asmx/GetArticulos</service_endpoint>
        <service_parameters>
            <service_parameter>
                <name>id_sade</name>
                <value></value>
            </service_parameter>
        </service_parameters>
    </articulo>
    <imagen>
        <destination_node>image</destination_node>
        <xml_file_path>/home/francort/xml/imagenes_comunicados.xml</xml_file_path>
        <x_path>/GetPressReleaseImage/image</x_path>
        <id_x_path>id_foto</id_x_path>
        <service_endpoint>http://10.0.17.152/sade_ws/service1.asmx/GetPressReleaseImage</service_endpoint>
        <service_parameters>
            <service_parameter>
                <name>id_sade</name>
                <value></value>
            </service_parameter>
            <service_parameter>
                <name>num_records</name>
                <value></value>
            </service_parameter>
        </service_parameters>
    </imagen>
</config>